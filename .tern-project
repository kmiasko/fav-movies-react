{
  "ecmaVersion": 6,
  "libs": [
    "browser"
  ],
  "loadEagerly": [
    "App.js",
    "src/components/**/*.jsx",
    "src/components/**/*.js",
    "src/core/**/*.js",
    "node_modules/react/**/*.js"
  ],
  "plugins": {
    "complete_strings": {
      "maxLength": 15
    },
    "node": {},
    "doc_comment": {
      "fullDocs": true,
      "strong": true
    }
  }
}
